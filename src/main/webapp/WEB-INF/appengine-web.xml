<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>qr2mobile</application>
    <version>3</version>
    <threadsafe>true</threadsafe>

    <sessions-enabled>true</sessions-enabled>

    <static-files>
        <include path="/favicon.ico" expiration="7d 0h" />
        <include path="/*.html" expiration="1h" />
        <include path="/content/pages/**.html" expiration="1h" />
        <include path="/css/**.css" expiration="7d 0h" />
        <include path="/js/**.js" expiration="7d 0h" />
        <include path="/**.png" expiration="7d 0h" />
        <include path="/**.jpg" expiration="7d 0h" />
    </static-files>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>
</appengine-web-app>
